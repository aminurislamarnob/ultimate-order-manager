{
  "name": "aiarnob/ultimate-order-manager",
  "description": "Add extra column on order list table. Add unlimited order status. Customer wise order list.",
  "license": "GPLv2 or later",
  "autoload": {
    "psr-4": {
      "AiArnob\\UltimateOrderManager\\": "includes/"
    }
  },
  "authors": [
    {
      "name": "Aminur Islam Arnob",
      "email": "aminur.arnob@gmail.com",
      "homepage": "https://wordpress.org/plugins/ultimate-woo-order-manager/"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "wp-coding-standards/wpcs": "dev-develop",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "tareq1988/wp-php-cs-fixer": "dev-master",
    "phpcompatibility/phpcompatibility-wp": "dev-master",
    "phpunit/phpunit": "~8.5",
    "wp-phpunit/wp-phpunit": "^6.1",
    "yoast/phpunit-polyfills": "^1.0",
    "symfony/var-dumper": "6.4.x-dev"
  },
  "scripts": {
    "phpcs": [
      "vendor/bin/phpcs -p -s"
    ],
    "phpcs:report": [
      "vendor/bin/phpcs --report-file='phpcs-report.txt'"
    ],
    "phpcbf": [
      "vendor/bin/phpcbf -p"
    ]
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "foroco/php-browser-detection": "dev-master"
  }
}
